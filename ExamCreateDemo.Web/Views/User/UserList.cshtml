

@model IEnumerable<User>


<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>

            <th style="width:30px">#</th>
            <th>User Name</th>
            <th style="width:200px">Full Name</th>
            <th style="width:160px">Email</th>
            <th style="width:30px"></th>
        </tr>
    </thead>
    <tbody>


        @foreach (var item in Model)
            {
        <tr>
            <td>@item.UserId</td>
            <td>@item.UserName</td>
            <td>@item.FullName</td>
            <td>@item.Email</td>
            <td>
                <a href="/user/UserDelete/@item.UserId" class="btn btn-danger btn-sm">Delete</a>
            </td>
        </tr>
        }


    </tbody>

</table>