
@model IEnumerable<Exam>

<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>

            <th style="width:30px">#</th>
            <th>Header</th>

            <th style="width:200px">Date</th>
            <th style="width:160px"></th>
            <th style="width:160px"></th>
        </tr>
    </thead>
    <tbody>


        @foreach (var item in Model)
        {
        <tr>
            <td>@item.ExamId</td>
            <td>@item.ExamArticleHeader</td>

            <td>@item.ExamCreatedDate</td>

            <td>
                <a href="/exam/ExamDetail/@item.ExamId" class="btn btn-success btn-sm">Go Exam</a>
            </td>
            <td>
                <a href="/exam/ExamDelete/@item.ExamId" class="btn btn-danger btn-sm">Delete</a>
            </td>



        </tr>
        }
        </tbody>

</table>



